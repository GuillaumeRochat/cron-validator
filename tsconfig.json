{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "./dist",
    "declaration": true,
    "noEmit": false,
    "sourceMap": true,
    "inlineSources": true,
